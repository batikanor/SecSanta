# Debug mode - set to true for mock data, false for real blockchain interaction
NEXT_PUBLIC_FE_DEBUG_MODE=true

# Sync server for automatic cross-browser syncing (DEBUG mode only)
# Set to true to enable automatic syncing across all browsers
NEXT_PUBLIC_USE_SYNC_SERVER=true
NEXT_PUBLIC_SYNC_SERVER_URL=http://localhost:3001

# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Chain configuration
NEXT_PUBLIC_CHAIN_ID=1
NEXT_PUBLIC_ENABLE_TESTNETS=true
